{
    "manifest_version": 2,
    "name": "Choice Extension",
    "description": "Browse guilt-free.",
    "version": "0.1",
    "incognito": "spanning",
    "permissions": ["tabs", "contextMenus"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["blocker/blocker.js"],
            "css": ["blocker/blocker.css"]
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}
